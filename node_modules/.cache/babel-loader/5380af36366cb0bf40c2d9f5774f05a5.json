AAA,IAAA,EAAA;AAAA,iBAAA;AAEA,qBAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AAhBA;AAAA;;AAAA;AAAA;AAAA;AAmBA,qBAAA,IAAA,CAAA,KAAA,CAAA,YAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AARA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- Header -->\r\n    <div class=\"header bg-gradient-success py-7 py-lg-8 pt-lg-9\">\r\n      <b-container>\r\n        <div class=\"header-body text-center mb-7\">\r\n          <b-row class=\"justify-content-center\">\r\n            <b-col xl=\"5\" lg=\"6\" md=\"8\" class=\"px-5\">\r\n              <h1 class=\"text-white\">Willkommen!</h1>\r\n              <p class=\"text-lead text-white\">Logge dich hier ein, um Zugriff zum Webinterface zu bekommen.</p>\r\n            </b-col>\r\n          </b-row>\r\n        </div>\r\n      </b-container>\r\n      <div class=\"separator separator-bottom separator-skew zindex-100\">\r\n        <svg x=\"0\" y=\"0\" viewBox=\"0 0 2560 100\" preserveAspectRatio=\"none\" version=\"1.1\"\r\n             xmlns=\"http://www.w3.org/2000/svg\">\r\n          <polygon class=\"fill-default\" points=\"2560 0 2560 100 0 100\"></polygon>\r\n        </svg>\r\n      </div>\r\n    </div>\r\n    <!-- Page content -->\r\n    <b-container class=\"mt--8 pb-5\">\r\n      <b-row class=\"justify-content-center\">\r\n        <b-col lg=\"5\" md=\"7\">\r\n          <b-card no-body class=\"bg-secondary border-0 mb-0\">\r\n            <b-card-header class=\"bg-transparent pb-5\"  >\r\n              <div class=\"text-muted text-center mt-2 mb-3\"><small>Einloggen mit</small></div>\r\n              <div class=\"btn-wrapper text-center\">\r\n                <!--\r\n                <a href=\"#\" class=\"btn btn-neutral btn-icon\">\r\n                  <span class=\"btn-inner--icon\"><img src=\"img/icons/common/github.svg\"></span>\r\n                  <span class=\"btn-inner--text\">Github</span>\r\n                </a>-->\r\n                <a href=\"#\" class=\"btn btn-neutral btn-icon\">\r\n                  <span class=\"btn-inner--icon\"><img src=\"img/icons/common/google.svg\"></span>\r\n                  <span class=\"btn-inner--text\">Google</span>\r\n                </a>\r\n              </div>\r\n            </b-card-header>\r\n            <b-card-body class=\"px-lg-5 py-lg-5\">\r\n              <div class=\"text-center text-muted mb-4\">\r\n                <small>Oder mit Zugangsdaten einloggen</small>\r\n              </div>\r\n              <validation-observer v-slot=\"{handleSubmit}\" ref=\"formValidator\">\r\n                <b-form role=\"form\" @submit.prevent=\"handleSubmit(loginUser)\">\r\n                  <base-input alternative\r\n                              class=\"mb-3\"\r\n                              name=\"E-Mail\"\r\n                              :rules=\"{required: true, email: true}\"\r\n                              prepend-icon=\"ni ni-email-83\"\r\n                              placeholder=\"E-Mail\"\r\n                              v-model=\"email\">\r\n                  </base-input>\r\n\r\n                  <base-input alternative\r\n                              class=\"mb-3\"\r\n                              name=\"Passwort\"\r\n                              :rules=\"{required: true, min: 8}\"\r\n                              prepend-icon=\"ni ni-lock-circle-open\"\r\n                              type=\"password\"\r\n                              placeholder=\"Passwort\"\r\n                              v-model=\"password\">\r\n                  </base-input>\r\n\r\n                  <b-form-checkbox v-model=\"rememberMe\">Remember me</b-form-checkbox>\r\n                  <div class=\"text-center\">\r\n                    <base-button type=\"primary\" native-type=\"submit\" class=\"my-4\">Einloggen</base-button>\r\n                  </div>\r\n                </b-form>\r\n              </validation-observer>\r\n            </b-card-body>\r\n          </b-card>\r\n          <b-row class=\"mt-3\">\r\n            <b-col cols=\"6\">\r\n              <router-link to=\"/dashboard\" class=\"text-light\"><small>Passwort vergessen?</small></router-link>\r\n            </b-col>\r\n            <b-col cols=\"6\" class=\"text-right\">\r\n              <router-link to=\"/register\" class=\"text-light\"><small>Du hast noch keinen Account?</small></router-link>\r\n            </b-col>\r\n          </b-row>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n  </div>\r\n</template>\r\n<script>\r\nimport AuthService from '../../services/AuthService'\r\n\r\nexport default {\r\n    data() {\r\n      return {\r\n          email: '',\r\n          password: '',\r\n          rememberMe: false\r\n      };\r\n    },\r\n    methods: {\r\n     async loginUser() {\r\n         try {\r\n          const credentials = {\r\n            password: this.password,\r\n            email: this.email,\r\n          };\r\n          const response = await AuthService.loginUser(credentials);\r\n\r\n          this.$awn.success(response.message);\r\n\r\n          const token = response.token;\r\n          const user = response.user;\r\n\r\n          this.$store.dispatch('auth/login', { token, user });\r\n\r\n          this.$router.push('/');\r\n\r\n        } catch (error) {\r\n          this.$awn.alert(error.response.data.response.message);\r\n        }\r\n      }\r\n    }\r\n  };\r\n</script>\r\n"],"sourceRoot":"src/views/Pages"}]}