,GAAA,CAAA;AACA,SAAA,eAAA,CAAA,CAAA;AACA,SAAA,UAAA;AACA,GA3BA;AA4BA,EAAA,SA5BA,uBA4BA;AACA,IAAA,aAAA,CAAA,KAAA,IAAA,CAAA;AACA,SAAA,KAAA,GAAA,CAAA;AACA,GA/BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,IAAA,WAAA,EACA;AACA,WAAA,KAAA,GAAA,CAAA;AACA,WAAA,UAAA;AACA,WAAA,IAAA,GAAA,UAAA,CAAA,YAAA;AAAA,aAAA,SAAA;AAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA,IAAA,CAAA;AACA,KAPA;AAQA,IAAA,UARA,wBAQA;AAAA;;AACA,MAAA,cAAA,CAAA,UAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,QAAA,CAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA,QAAA,CAAA,CAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,KAAA,CAAA,cAAA,CAAA,KAAA,CAAA,CAAA,EAAA,EAAA,CAAA;AACA,YAAA,KAAA,CAAA,KAAA,IAAA,CAAA,EACA,KAAA,CAAA,SAAA;AACA,OANA;AAOA,KAhBA;AAiBA,IAAA,eAjBA,2BAiBA,KAjBA,EAiBA;AACA,WAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,QAAA,CAAA,CAAA;AACA,WAAA,cAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA,KAAA,GAAA,CAAA,IAAA,KAAA,OAAA;AACA,WAAA,cAAA,GAAA,KAAA,cAAA,CAAA,KAAA,CAAA,CAAA,EAAA,EAAA,CAAA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,KAtBA;AAuBA,IAAA,UAvBA,wBAuBA;AAAA;;AACA,WAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,QAAA,CAAA,CAAA;AACA,WAAA,cAAA,GAAA,KAAA,QAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,eAAA,IAAA,CAAA,SAAA,CAAA,CAAA,EAAA,QAAA,GAAA,WAAA,GAAA,QAAA,CAAA,MAAA,CAAA,MAAA,CAAA,QAAA,GAAA,WAAA,EAAA,CAAA;AAAA,OAAA,CAAA;AACA,WAAA,cAAA,GAAA,KAAA,cAAA,CAAA,KAAA,CAAA,CAAA,EAAA,EAAA,CAAA;AACA;AA3BA;AAhCA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n    <base-header class=\"pb-6 pb-8 pt-5 pt-md-8 bg-gradient-success\">\r\n      <dashboard-stats/>\r\n     </base-header>\r\n\r\n     <b-container fluid class=\"mt--7\">\r\n      <b-row>\r\n        <b-col>\r\n    <b-card no-body>\r\n        <b-card-header class=\"border-0\">\r\n                <!-- Stack the columns on mobile by making one full-width and the other half-width -->\r\n                <b-row>\r\n            <b-col lg=\"8\" >\r\n            <h3 >Meine Produkte</h3>\r\n            </b-col>\r\n            <b-col lg=\"4\">\r\n            <base-input\r\n                      type=\"text\"\r\n                      placeholder=\"Suchen\"\r\n                      @keyup=\"searchItem()\"\r\n                      v-model=\"search\"\r\n                    />\r\n            </b-col>\r\n                </b-row>\r\n        </b-card-header>\r\n\r\n        <el-table class=\"table-responsive table\"\r\n                  header-row-class-name=\"thead-light\"\r\n                  :data=\"currentContent\">\r\n\r\n            <el-table-column label=\"ID\"\r\n                             min-width=\"260px\"\r\n                             prop=\"name\">\r\n                <template v-slot=\"{row}\">\r\n                            <span v-if=\"row.vserver != null\" class=\"font-weight-600 name mb-0 text-sm\">{{row.vserver._id}}</span>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column label=\"Status\"\r\n                             min-width=\"130px\"\r\n                             prop=\"status\">\r\n                <template v-slot=\"{row}\">\r\n                    <badge v-if=\"row.vserver != null\" class=\"badge-dot mr-4\" type=\"\">\r\n                        <badge v-if=\"!row.vserver.blocked\" class=\"badge-dot mr-4\" type=\"\">\r\n                            <badge v-if=\"row.vserver.status == 'Online'\" class=\"badge-dot mr-4\" type=\"\">\r\n                                <i :class=\"`bg-success`\"></i>\r\n                                <span class=\"status\" :class=\"`text-success`\">{{row.vserver.status}}</span>\r\n                            </badge>\r\n                            <badge v-if=\"row.vserver.status == 'Installation'\" class=\"badge-dot mr-4\" type=\"\">\r\n                                <i :class=\"`bg-warning`\"></i>\r\n                                <span class=\"status\" :class=\"`text-warning`\">{{row.vserver.status}}</span>\r\n                            </badge>\r\n                            <badge v-if=\"row.vserver.status != 'Online' && row.vserver.status != 'Installation'\" class=\"badge-dot mr-4\" type=\"\">\r\n                                <i :class=\"`bg-danger`\"></i>\r\n                                <span class=\"status\" :class=\"`text-danger`\">{{row.vserver.status}}</span>\r\n                            </badge>\r\n                        </badge>\r\n                        <badge v-else class=\"badge-dot mr-4\" type=\"\">\r\n                            <i :class=\"`bg-danger`\"></i>\r\n                            <span class=\"status\" :class=\"`text-danger`\">Gesperrt</span>\r\n                        </badge>\r\n                    </badge>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column label=\"Produkt\" min-width=\"150px\">\r\n                <template v-slot=\"{row}\">\r\n                    <badge v-if=\"row.vserver != null\" class=\"badge-dot mr-4\" style=\"color: #606266\" type=\"\">\r\n                        VServer\r\n                    </badge>\r\n                </template>\r\n            </el-table-column>\r\n\r\n             <el-table-column label=\"Kosten\" min-width=\"170px\">\r\n                <template v-slot=\"{row}\">\r\n                    <badge v-if=\"row.vserver != null\" class=\"badge-dot mr-4\" style=\"color: #606266\" type=\"\">\r\n                        {{ row.vserver.price }} € / 30 Tage\r\n                    </badge>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column label=\"Bezahlt bis\" min-width=\"180px\">\r\n                <template v-slot=\"{row}\">\r\n                    <badge v-if=\"row.vserver != null\" class=\"badge-dot mr-4\" style=\"color: #606266\" type=\"\">\r\n                        {{ row.vserver.paidup | moment('DD.MM.YYYY, HH:mm') }} \r\n                    </badge>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column label=\"Öffnen\" min-width=\"120px\">\r\n                <template v-slot=\"{row}\">\r\n                    <router-link v-if=\"row.vserver != null && row.vserver.status != 'Installation'\" :to=\"'/vserver/' + row.vserver._id\" custom v-slot=\"{ navigate }\">\r\n                        <a @click=\"navigate\" @keypress.enter=\"navigate\" role=\"link\" style=\"cursor: pointer; font-size: 1.1rem;\"><i class=\"ni ni-active-40\" aria-hidden=\"true\"></i></a>\r\n                    </router-link>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <b-card-footer class=\"py-4 d-flex justify-content-end\">\r\n            <base-pagination @change=\"value => changeTableView(value)\" v-model=\"currentPage\" :per-page=\"perpage\" :total=\"total\"></base-pagination>\r\n        </b-card-footer>\r\n    </b-card>\r\n        </b-col>\r\n      </b-row>\r\n      </b-container>\r\n    </div>\r\n</template>\r\n<script>\r\nimport products from '../Tables/default';\r\nimport VServerService from '../../services/VServerService';\r\nimport DashboardStats from '../Layout/DashboardStats.vue';\r\n  import { Table, TableColumn, DropdownMenu, DropdownItem, Dropdown} from 'element-ui'\r\nimport Badge from '../../components/Badge.vue';\r\n\r\n  export default {\r\n    name: 'light-table',\r\n    components: {\r\n      [Table.name]: Table,\r\n      [TableColumn.name]: TableColumn,\r\n      [Dropdown.name]: Dropdown,\r\n      [DropdownItem.name]: DropdownItem,\r\n      [DropdownMenu.name]: DropdownMenu,\r\n      DashboardStats,\r\n        Badge,\r\n    },\r\n    data() {\r\n      return {\r\n        products,\r\n        currentContent: products,\r\n        currentPage: 1,\r\n        perpage: 10,\r\n        total: products.length,\r\n        search: '',\r\n        loop: null,\r\n        loopi: 0,\r\n      };\r\n    },\r\n    created() {\r\n        this.currentPage = 1;\r\n        this.changeTableView(1);\r\n        this.getServers();\r\n    },\r\n    destroyed () {\r\n        clearInterval(this.loop);\r\n        this.loopi = 0;\r\n    },\r\n    methods: {\r\n        startLoop() {\r\n            if(this.$route.path != '/products')\r\n                return;\r\n            this.loopi = 1;\r\n            this.getServers();      \r\n            this.loop = setTimeout(function () { this.startLoop() }.bind(this), 4000)\r\n        },\r\n        getServers() {\r\n            VServerService.getServers().then(response => {\r\n                this.products = response.vservers.reverse();\r\n                this.currentContent = JSON.parse(JSON.stringify(this.products));\r\n                this.currentContent = this.currentContent.slice(0, 10)\r\n                if(this.loopi == 0)\r\n                    this.startLoop();\r\n            })\r\n        },\r\n        changeTableView(value) {\r\n            this.currentContent = JSON.parse(JSON.stringify(this.products));\r\n            this.currentContent.splice(0, ((value - 1) * this.perpage));\r\n            this.currentContent = this.currentContent.slice(0, 10)\r\n            this.search = ''\r\n        },\r\n        searchItem() {\r\n            this.currentContent = JSON.parse(JSON.stringify(this.products));\r\n            this.currentContent = this.products.filter(i => JSON.stringify(i).toString().toLowerCase().includes(this.search.toString().toLowerCase()));\r\n            this.currentContent = this.currentContent.slice(0, 10)\r\n        }\r\n    },\r\n  }\r\n</script>"],"sourceRoot":"src/views/Pages"}]}