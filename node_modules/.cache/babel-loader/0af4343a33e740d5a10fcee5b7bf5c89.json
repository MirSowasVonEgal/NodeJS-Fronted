      commit('SET_TOKEN', token);\r\n      commit('SET_USER', user);\r\n      state.token = token;\r\n      state.user = user;\r\n      localStorage.setItem('token', token)\r\n      // set auth header\r\n      Axios.defaults.headers.common['Authorization'] = `Bearer ${token}`;\r\n    },\r\n    logout: ({ commit }) => {\r\n      commit('RESET', '');\r\n      localStorage.removeItem('token');\r\n      router.push(\"/login\")\r\n    },\r\n    user: ({ commit, dispatch }, user) => {\r\n      commit('SET_USER', user);\r\n    },\r\n}\r\n\r\nexport default {\r\n    namespaced: true,\r\n    getters,\r\n    state,\r\n    actions,\r\n    mutations,\r\n}"]}]}