AA,UAAA,QAAA,EAAA;AACA,QAAA,WAAA,CAAA,UAAA,GAAA,IAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,IAAA,CAAA;AAAA,SAAA;;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,QAAA,CAAA,OAAA;AACA,OAHA,EAGA,KAHA,CAGA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA;AACA,OALA;AAMA;AACA,GA3BA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;;AACA,UAAA,KAAA,MAAA,CAAA,SAAA,IAAA,QAAA,EAAA;AACA,QAAA,aAAA,CAAA,iBAAA,CAAA,KAAA,MAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,QAAA,CAAA,GAAA;AACA,SAFA,EAEA,KAFA,CAEA,UAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA;AACA,SALA;AAMA;AACA;AAXA;AA5BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n     <base-header class=\"pb-6 pb-8 pt-5 pt-md-8 bg-gradient-success\">\r\n      <dashboard-stats/>\r\n     </base-header>\r\n\r\n    <b-container fluid class=\"mt--6\">\r\n      <b-row>\r\n        <b-col xl=\"12\" class=\"order-xl-1\">\r\n          <card>\r\n          <b-row align-v=\"center\" slot=\"header\" >\r\n            <b-col cols=\"8\">\r\n              <h3 class=\"mb-0\">Guthaben aufladen</h3>\r\n            </b-col>\r\n          </b-row>\r\n\r\n          <b-form @submit.prevent=\"updateProfile\">\r\n            <h6 class=\"heading-small text-muted mb-4\">Account Informationen</h6>\r\n\r\n              <div class=\"pl-lg-4\">\r\n                <b-row>\r\n                  <b-col lg=\"6\">\r\n                    <base-input\r\n                      type=\"text\"\r\n                      label=\"Nutzername\"\r\n                      placeholder=\"Dein Nutzername\"\r\n                      v-model=\"user.username\"\r\n                      disabled\r\n                    >\r\n                    </base-input>\r\n                  </b-col>\r\n                  <b-col lg=\"6\">\r\n                    <base-input\r\n                      type=\"email\"\r\n                      label=\"E-Mail Adresse\"\r\n                      placeholder=\"Deine E-Mail\"\r\n                      v-model=\"user.email\"\r\n                      disabled\r\n                    >\r\n                    </base-input>\r\n                  </b-col>\r\n                </b-row>\r\n              </div>\r\n              <hr class=\"my-4\">\r\n\r\n              <!-- Address -->\r\n              <h6 class=\"heading-small text-muted mb-4\">Guthaben aufladen</h6>\r\n\r\n              <div class=\"pl-lg-4\">\r\n                <b-row>\r\n                  <b-col md=\"6\" style=\"width: 100%\">\r\n                      <label class=\"form-control-label\">Zahlungsmethode</label>\r\n                         <select class=\"form-control\" v-model=\"charge.paymethod\" aria-label=\"Default select example\">\r\n                            <option selected value=\"paypal\">PayPal</option>\r\n                        </select>\r\n                  </b-col>\r\n                   <b-col md=\"6\">\r\n                    <base-input\r\n                      type=\"number\"\r\n                      label=\"Betrag \"\r\n                      placeholder=\"5.00\"\r\n                      v-model=\"charge.amount\"\r\n                    >\r\n                    </base-input>\r\n                  </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                  <b-col md=\"8\">\r\n                    \r\n                  </b-col>\r\n                   <b-col md=\"4\">\r\n                        <vue-element-loading :active=\"loading\" spinner=\"bar-fade-scale\" color=\"#2dce89\"/>\r\n                        <a @click=\"chargeUser()\" style=\"width: 100%; cursor: pointer; color: white;\" class=\"btn btn-success\"><b>Jetzt aufladen!</b></a>\r\n                  </b-col>\r\n                </b-row>\r\n              </div>\r\n            </b-form>\r\n          </card>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport DashboardStats from '../Layout/DashboardStats.vue';\r\nimport ChargeService from '../../services/ChargeService';\r\nimport AuthService from '../../services/AuthService';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  components: { DashboardStats },\r\n  computed: {\r\n    ...mapGetters({ user: \"auth/getUser\" })\r\n  },\r\n  data() {\r\n    return {\r\n      charge: {\r\n        amount: '3.00',\r\n        paymethod: 'paypal'\r\n      },\r\n      loading: false\r\n    };\r\n  },\r\n  beforeCreate() {\r\n    if(this.$route.query.method == 'paypal') {\r\n      const credentials = {\r\n        paymentid: this.$route.query.paymentId,\r\n        payerid: this.$route.query.PayerID,\r\n      };\r\n      ChargeService.chargeUserPayPal(credentials).then(response => {\r\n        AuthService.getProfile().then(user => this.$store.dispatch(\"auth/user\", user));\r\n        this.$awn.success(response.message);\r\n      }).catch(error => {\r\n        this.$awn.alert(error.response.data.response.message);\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    chargeUser() {\r\n      this.loading = true;\r\n      if(this.charge.paymethod == 'paypal') {\r\n        ChargeService.generateURLPayPal(this.charge.amount).then(response => {\r\n            window.location.href = response.url;\r\n        }).catch(error => {\r\n            this.loading = false;\r\n            this.$awn.alert(error.response.data.response.message);\r\n        });\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/Pages"}]}