/b-row>\r\n       </div>\r\n         <b-navbar-nav  class=\"align-items-lg-center ml-lg-auto\">\r\n           <li class=\"nav-item\">\r\n            <router-link style=\"cursor: pointer;\" class=\"nav-link\" to=\"/register\" custom v-slot=\"{ navigate }\">\r\n                <i class=\"ni ni-circle-08\"></i>\r\n                <a href=\"\" class=\"font-weight-bold text-white mt-5\" @click=\"navigate\" @keypress.enter=\"navigate\" role=\"link\">Registrieren</a>\r\n            </router-link>\r\n           </li>\r\n           <li class=\"nav-item\">\r\n            <router-link style=\"cursor: pointer;\" class=\"nav-link\" to=\"/login\" custom v-slot=\"{ navigate }\">\r\n                <a href=\"\" class=\"font-weight-bold text-white mt-5\" @click=\"navigate\" @keypress.enter=\"navigate\" role=\"link\">Einloggen</a>\r\n            </router-link>\r\n           </li>\r\n       </b-navbar-nav>\r\n     </template>\r\n    </base-nav>\r\n\r\n    <div class=\"main-content\">\r\n      <zoom-center-transition\r\n        :duration=\"pageTransitionDuration\"\r\n        mode=\"out-in\"\r\n      >\r\n        <router-view></router-view>\r\n      </zoom-center-transition>\r\n    </div>\r\n\r\n    <footer class=\"py-5\" id=\"footer-main\">\r\n      <b-container >\r\n        <b-row align-v=\"center\" class=\"justify-content-xl-between\">\r\n          <b-col xl=\"6\">\r\n            <div class=\"copyright text-center text-xl-left text-muted\">\r\n              © {{year}} <a href=\"#\" class=\"font-weight-bold ml-1\">ShadeHost.eu</a>\r\n            </div>\r\n          </b-col>\r\n          <b-col xl=\"6\" class=\"col-xl-6\">\r\n            <b-nav  class=\"nav-footer justify-content-center justify-content-xl-end\">\r\n              <b-nav-item href=\"#\" target=\"_blank\">\r\n                AGB\r\n              </b-nav-item>\r\n              <b-nav-item href=\"#\"  target=\"_blank\">\r\n                Impressum\r\n              </b-nav-item>\r\n              <b-nav-item href=\"#\" targe